{
  "name": "@moco-comics/server",
  "private": true,
  "version": "1.0.0",
  "description": "A Strapi application",
  "scripts": {
    "dev": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "lint": "eslint . --ignore-path .gitignore",
    "prettier": "prettier . --ignore-path .gitignore",
    "check-format": "yarn prettier --list-different",
    "format": "yarn prettier --write",
    "validate": "yarn check-format && yarn lint && yarn build",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "knex": "<0.20.0",
    "sqlite3": "latest",
    "strapi": "3.3.2",
    "strapi-admin": "3.3.2",
    "strapi-connector-bookshelf": "3.3.2",
    "strapi-plugin-content-manager": "3.3.2",
    "strapi-plugin-content-type-builder": "3.3.2",
    "strapi-plugin-email": "3.3.2",
    "strapi-plugin-upload": "3.3.2",
    "strapi-plugin-users-permissions": "3.3.2",
    "strapi-utils": "3.3.2"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "1a644139-c743-483a-b854-16e6fa8333dc"
  },
  "engines": {
    "node": ">=10.16.0 <=14.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.+(js|json)": [
      "prettier --write",
      "prettier --list-different"
    ],
    "*.js": [
      "eslint"
    ]
  }
}
